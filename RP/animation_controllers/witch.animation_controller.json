{
	"format_version": "1.21.90",
	"animation_controllers": {
		"controller.animation.hsmp_witch": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"blink": "query.anim_time('default') > 2.8"
						},
						{
							"next_page": "query.anim_time('default') > 2.8"
						}
					]
				},
				"blink": {
					"animations": [
						"blink",
						"idle"
					],
					"transitions": [
						{
							"default": "true"
						}
					]
				},
				"next_page": {
					"animations": [
						"next_page",
						"idle"
					],
					"transitions": [
						{
							"default": "true"
						}
					]
				}
			}
		}
	}
}